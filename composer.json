{
    "name": "imanghafoori/crudbooster-notifications",
    "description": "A plug-in for CrudBooster CMS to add notifications functionality",
    "keywords": ["crudbooster", "crudbooster-notifications", "crudbooster notifications", "crudbooster plugin"],
    "license": "MIT",
    "homepage": "https://github.com/imanghafoori1/crudbooster-notifications",
    "authors": [
        {
            "name": "Iman Ghafoori",
            "email": "imanghafoori1@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "crocodicstudio/crudbooster": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Imanghafoori\\CrudBoosterNotifications\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imanghafoori\\CrudBoosterNotifications\\CbNotificationsServiceProvider"
            ]
        }
    }
}
